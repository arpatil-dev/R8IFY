				{
					"name": "Login User",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"try {\r",
									"  const res = pm.response.json();\r",
									"  const token = res.token || res.accessToken || res.authToken || (res.data && (res.data.token || res.data.accessToken));\r",
									"  if (token) {\r",
									"    pm.environment.set(\"authToken\", token);\r",
									"    console.log(\"Saved authToken\");\r",
									"  } else {\r",
									"    console.log(\"No auth token found in response. Response keys:\", Object.keys(res));\r",
									"  }\r",
									"} catch (e) {\r",
									"  console.log(\"Response is not JSON or parsing failed:\", e);\r",
									"}\r",
									""
								],
								"type": "text/javascript",
								"packages": {}
							}
						},
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"if (pm.environment.get(\"authToken\")) {\r",
									"    pm.request.headers.add({\r",
									"        key: \"Authorization\",\r",
									"        value: \"Bearer \" + pm.environment.get(\"authToken\")\r",
									"    });\r",
									"}\r",
									""
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
